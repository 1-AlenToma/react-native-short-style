{
  "name": "react-native-short-style",
  "version": "1.2.9",
  "description": "#react-native-short-style",
  "author": "1-AlenToma",
  "type": "module",
  "license": "MIT",
  "repository": "https://github.com/1-AlenToma/react-native-short-style",
  "source": "index.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "css in js",
    "css3",
    "css in react-native",
    "react-native css"
  ],
  "scripts": {
    "clean": "rmdir /s /q dist",
    "build": "tsc && cd demo && npm run export",
    "assets": "cpx \"demo/assets/**/*\" dist/demo/assets && cpx \"demo/src/assets/**/*\" dist/demo/src/assets",
    "ready": "npm run clean && npm run build && npm run assets",
    "web": "cd demo && npx expo start",
    "test": "tsc  && jest tsx ./dist/test/Methods.test.js"
  },
  "peerDependencies": {
    "msgpackr": "*",
    "react": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "@types/node": "*",
    "@types/react": "*",
    "@types/react-native": "*",
    "cpx": "*",
    "typescript": "*"
  },
  "files": [
    "dist",
    "./"
  ],
  "dependencies": {
    "@miblanchard/react-native-slider": "*",
    "react-native-short-style-devtools": "*",
    "react-smart-state": "*"
  }
}
