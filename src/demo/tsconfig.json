{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "module": "esnext",
    "outDir": "public",
    "strict": true,
    "noImplicitThis": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "skipLibCheck": true
  },
  "include": [
    "./"
  ],
  "exclude": [
    "node_module"
  ]
}