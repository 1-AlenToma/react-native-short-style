{
  "name": "react-native-short-style",
  "version": "1.2.1",
  "description": "#react-native-short-style",
  "author": "1-AlenToma",
  "type": "module",
  "license": "MIT",
  "repository": "https://github.com/1-AlenToma/react-native-short-style",
  "source": "index.ts",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build": "rmdir /s /q dist && npm run build:cjs && npm run build:esm",
    "web": "cd demo && npx expo start",
    "test": "tsc  && jest tsx ./dist/test/Methods.test.js"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "@types/node": "*",
    "@types/react": "*",
    "@types/react-native": "*",
    "typescript": "*"
  },
  "files": [
    "dist",
    "./"
  ],
  "dependencies": {
    "@miblanchard/react-native-slider": "*",
    "css-select": "*",
    "react-native-get-random-values": "*",
    "react-smart-state": "*"
  }
}